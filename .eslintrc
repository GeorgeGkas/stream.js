{
  "extends": ["airbnb-base"],
  "parserOptions": {
    "sourceType": "script"
  },
  "env": {
    "jasmine": true
  },
  "rules": {
    "strict": [2, "global"],
    "object-shorthand": [2, "consistent"],
    "func-names": [2, "never"],
    "no-param-reassign": 0,
    "prefer-arrow-callback": 0,
    "prefer-rest-params": 0,
    "prefer-spread": 0,
    "prefer-destructuring": 0,
    "prefer-template": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "no-console": 0
  }
}
